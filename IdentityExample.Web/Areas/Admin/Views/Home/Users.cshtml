@{
    ViewData["Title"] = "Users";
}
@model List<AdminViewModel.UserViewModel>
<h1>Users</h1>
<div class="row">
    <div class="col-md-12">
        <table class="table">
            <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">UserName</th>
                <th scope="col">Email</th>
            </tr>
            </thead>
            <tbody>
            @foreach (AdminViewModel.UserViewModel item in Model)
            {
                <tr>
                    <th scope="row">@item.Id</th>
                    <td>@item.UserName</td>
                    <td>@item.Email</td>
                </tr>
            }
            </tbody>
        </table>
    </div>
</div>